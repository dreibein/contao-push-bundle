{
    "name": "dreibein/contao-push-bundle",
    "type": "contao-bundle",
    "description": "Add Push Notifications to your Contao installation",
    "homepage": "https://www.agentur-dreibein.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Agentur Dreibein",
            "homepage": "https://www.agentur-dreibein.de",
            "email": "development@agentur-dreibein.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/dreibein/contao-push-bundle/issues",
        "source": "https://github.com/dreibein/contao-push-bundle"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "contao/core-bundle": "^4.9",
        "contao/news-bundle": "^4.9",
        "minishlink/web-push-bundle": "^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpunit/phpunit": "^9.5"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "extra": {
        "contao-manager-plugin": "Dreibein\\ContaoPushBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Dreibein\\ContaoPushBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dreibein\\ContaoPushBundle\\Tests\\": "tests/"
        }
    }
}
